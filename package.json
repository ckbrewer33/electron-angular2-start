{
  "name": "electron-angular2-start",
  "version": "0.1.0",
  "description": "A starting point for an Electron application using Angular 2",
  "main": "main.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"electron .\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ckbrewer33/electron-angular2-start"
  },
  "keywords": [
    "Electron",
    "Angular2"
  ],
  "author": "Chris Brewer",
  "license": "MIT",
  "homepage": "https://github.com/ckbrewer33/electron-angular2-start",
  "dependencies": {
    "@angular/common": "^2.1.2",
    "@angular/compiler": "^2.1.2",
    "@angular/core": "^2.1.2",
    "@angular/forms": "^2.1.2",
    "@angular/http": "^2.1.2",
    "@angular/platform-browser": "^2.1.2",
    "@angular/platform-browser-dynamic": "^2.1.2",
    "@angular/router": "^3.1.2",
    "@angular/upgrade": "^2.1.2",
    "core-js": "^2.4.1",
    "electron": "^1.4.10",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "typescript": "^2.1.1",
    "typings": "^1.5.0",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "concurrently": "^2.2.0"
  }
}
